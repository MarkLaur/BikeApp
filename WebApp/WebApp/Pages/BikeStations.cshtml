@page
@model WebApp.Pages.BikeStationsModel
@{
    ViewData["Title"] = "Bike Trips";
}

<h2>List of bike stations</h2>

<table>
    <tr>
        <th>Station ID</th>
        <th>Name</th>
    </tr>

    @foreach (Models.Station station in Model.BikeStations)
    {
            <tr>
                <td>@station.ID</td>
                <td>@station.Name</td>
            </tr>
    }
</table>